\newcommand{\ASI}[2]{
    \ifnum \value{#1}<4 {%
        \addtocounter{#1}{1}%
    }
    \else {%
        \iftoggle{#2}{%
            \global\togglefalse{#2}
            \addtocounter{#1}{1}%
            \setcounter{#2}{\value{#1}*2+10}%
        }{%
            \global\toggletrue{#2}
            \setcounter{#2}{\value{#1}*2+11}%
        }
    }
    \fi
}


