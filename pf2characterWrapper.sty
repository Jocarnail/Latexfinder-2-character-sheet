    key ability score/wisdom/.code = \setcounter{RangedIIIKey}{\value{MSag}},
    key ability score/charisma/.code = \setcounter{RangedIIIKey}{\value{MCar}},
    damage type/.store in = \RangedIIIDmgType,
    wspec/.code = \setbool{RangedIIIWSpec}{true}
}

\newcommand{\setWeapon}[1]{
